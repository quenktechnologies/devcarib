{

    "type": "object",

    "title": "Admin",

    "collection": "admins",

    "validator:imports": {

        "@board/types/lib/admin": "Admin"

    },

    "checks:imports": {

        "@board/validation/lib/admin": ["validate", "validatePartial"]

    },

    "properties": {

        "id": {

            "type": "number",

            "readOnly": true,

            "checks-complete": [

                ["./#inc", ["'counters.admins'"]],

                ["./#unique", ["'admins'", "'id'"]]

            ]

        },

        "name": {

            "type": "string",

            "validator": "./#name"

        },

        "email": {

            "type": "string",

            "validator": "./#email"

        },

        "password": {

            "type": "string",

            "validator": "./#password"

        }

    }

}
