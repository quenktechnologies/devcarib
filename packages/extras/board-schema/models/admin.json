{

    "type": "object",

    "title": "Admin",

    "collection": "admins",

    "validation:imports": {

        "@board/types/lib/admin": "Admin"

    },

    "checks:imports": {

        "@board/validation/lib/admin": ["validate", "validatePartial"]

    },

    "properties": {

        "id": {

            "type": "number",

            "readOnly": true,

            "checks": [

                ["./#inc", ["'counters.admin'"]],

                ["./#unique", ["'admins'", "'id'"]]

            ]

        },

        "name": {

            "type": "string",

            "validation": "./#name"

        },

        "email": {

            "type": "string",

            "validation": "./#email"

        },

        "password": {

            "type": "string",

            "validation": "./#password"

        }

    }

}
