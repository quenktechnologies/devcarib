
{% macro imports(doc) %}

  {% if doc.import %}
    {% for sym,mod in doc.import %}
      {% if (sym != doc.title) and ((not doc.definitions) or (not doc.definitions[sym])) %}
        import { {{sym}} }from '{{mod}}';{{'\n'}}
      {% else %}
          {{''}}
      {% endif %}
    {% endfor %} 
  {% endif %}

{% endmacro %}


