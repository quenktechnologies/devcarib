{% import (HeadView, HeadViewContext) from "./common/head" %}
{% import (HeaderView) from "./common/header" %}

{# JobFormView is the view for the "new job" form page. #}
{% view JobFormView (Object) %}

  {% let headCtx:HeadViewContext = {

    title = "Post a Job - Caribbean Developers",

    styles = ["/assets/css/job-form.css"]

  }%}

  <html>

    {{ <HeadView(headCtx)> }}

    <body>

      {{ <HeaderView(@)> }}

      <div id="main"></div>

      <script src="/assets/js/job-form.js"></script>

    </body>

  </html>
