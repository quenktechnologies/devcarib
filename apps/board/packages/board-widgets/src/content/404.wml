{% import (HeadView) from "./head" %}

{# NotFoundView is used when visitors hit 404 errors. #}
{% view NotFoundView (Object) %}

 <html>

  {{ <HeadView({title="Here does not exist"})> }}

  <body>

    <p>The page you requested does not exist (404). We promise to make this fancier someday.</p>

    <p>Click <a href="/">here</a> to return to the main site.</p>

  </body>

</html>
