{

    "type": "object",

    "title": "Admin",

    "collection": "admins",

    "model.type": "ModelType",

    "models:imports": {

        "$ref": "./common/imports.json",

        "@mia/types/lib/admin": {

            "Admin": "ModelType"

        },

        "@quenk/safe-mongodb/lib/database/collection": {

            "populate": "populate",

            "populateN": "populateN"

        }

    },

    "validators:imports": {

        "@mia/types/lib/admin": "Admin"

    },

    "checks:imports": {

        "@mia/validators/lib/admin": ["validate", "validatePartial"]

    },

    "properties": {

        "id": {

            "type": "number",

            "readOnly": true,

            "checks-complete": [

                ["@devcarib/common-checks#inc", ["'counters.admins'"]],

                ["@devcarib/common-checks#unique", ["'admins'", "'id'"]]

            ]

        },

        "name": {

            "type": "string",

            "validators": "@devcarib/common-validators#name"

        },

        "email": {

            "type": "string",

            "validators": "@devcarib/common-validators#email"

        },

        "password": {

            "type": "string",

            "validators": "@devcarib/common-validators#password",

            "checks": ["@devcarib/common-checks#bcrypt"]

        }

    }

}
