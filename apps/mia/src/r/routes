-- Admins
POST    /admins      api.create +model="admin"
GET     /admins      api.search +search="admin"

GET     /admins/:id  api.get
PATCH   /admins/:id  api.update +model="admin"
DELETE  /admins/:id  api.remove

-- Jobs
POST    /jobs        api.create +model="job"
GET     /jobs        api.search +search="job"

PATCH   /jobs/:id    api.update +model="job"
GET     /jobs/:id    api.get
DELETE  /jobs/:id    api.remove

-- Users
GET     /users       api.search +search="user"

PATCH   /users/:id   api.update +model="user"
GET     /users/:id   api.get
DELETE  /users/:id   api.remove

-- Events

POST    /events      api.create +model="event"
GET     /events      api.search +search="event"

PATCH   /events/:id  api.update +model="event"
GET     /events/:id  api.get
DELETE  /events/:id  api.remove

-- Invites

POST    /invites     api.create +model="invite"
GET     /invites     api.search +search="invite"

PATCH   /invites/:id api.update +model="invite"
GET     /invites/:id api.get
DELETE  /invites/:id api.remove
